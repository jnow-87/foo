#include <heartbeat.dts>


#ifndef HEARTBEAT_PIN
# error "heartbeat pin not defined"
#endif

heartbeat = {
	compatible = "kernel,heartbeat";

	heartbeat_dtd(
		HEARTBEAT_PIN,	// pin
		0x0505,			// wave
		200				// period [ms]
	)
};
