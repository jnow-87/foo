#include <config/config.h>
#include <gpio.dts>


port0 = {
	compatible = "x86,gpio";

	gpio0 = {
		compatible = "gpio,port";

		gpio_cfg_t(
			0xff,	// in mask
			0xff,	// out mask
			0x00,	// int mask
			0x00,	// invert mask
			0		// interrupt
		);
	};
};
