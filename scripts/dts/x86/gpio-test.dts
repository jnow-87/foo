#include <config/config.h>
#include <gpio.dts>


port-test = {
	compatible = "x86,gpio";

	gpio-port = {
		compatible = "gpio,port";

		gpio_cfg_t(
			0xff0f,	// in mask
			0xff00,	// out mask
			0x000f,	// int mask
			0x0005,	// invert mask
			0		// interrupt
		);
	};

	gpio-pin = {
		compatible = "gpio,pin";

		gpio_cfg_t(
			0xff,	// in mask
			0xff,	// out mask
			0x0f,	// int mask
			0x00,	// invert mask
			0		// interrupt
		);
	};
};
